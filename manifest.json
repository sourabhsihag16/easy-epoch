{
    "manifest_version": 3,
    "name": "Easy Epoch",
    "description": "",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "bubble.css"
            ],
            "js": [
                "jQuery.mini3.js",
                "selection.js"
            ],
            "all_frames": true
        }
    ]
}